{
    "name": "rossignoles/erp-ecole",
    "description": "Système ERP complet pour la gestion d'une école",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Hyacinthe",
            "email": "contact@rossignoles.com"
        }
